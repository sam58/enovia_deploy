<idea-plugin>
    <id>org.jetbrains</id>
    <name>Hello Action Project</name>
    <version>0.0.1</version>
    <vendor email="dummy" url="dummy">dummy</vendor>

    <depends>com.intellij.modules.lang</depends>

    <extensions defaultExtensionNs="com.intellij">
        <fileTypeFactory implementation="sam58.enovia_deploy.language.mxu.MXUFileTypeFactory"/>
    </extensions>

    <actions>
        <action class="sam58.enovia_deploy.actions.MxUpdate" id="sam58.enovia_deploy.actions.MxUpdate"
                text="MxUpdate" description="Setup .mxu files by mxUpdate tools. ">
            <add-to-group group-id="EditorPopupMenu" anchor="last"/>

        </action>

    </actions>


</idea-plugin>